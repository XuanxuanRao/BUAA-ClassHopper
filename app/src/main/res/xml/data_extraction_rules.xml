<?xml version="1.0" encoding="utf-8"?><!--
   Data extraction rules file for UUID persistence
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Include device info shared preferences for UUID persistence -->
        <include domain="sharedpref" path="device_info.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Include device info shared preferences for UUID persistence during device transfer -->
        <include domain="sharedpref" path="device_info.xml"/>
    </device-transfer>
</data-extraction-rules>